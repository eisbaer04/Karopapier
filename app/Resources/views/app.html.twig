{% extends 'base.html.twig' %}

{% block javascripts %}
    <script>
        window.turtedHost = '{{ getConfig('turted') }}';
    </script>
    {% if app.environment == "prod" %}
        <script src="{{ cachebust('/js/libs.min.js') }}"></script>
        <script src="{{ cachebust('/js/KaroApp.min.js') }}"></script>
    {% else %}
        <script src="{{ cachebust('/js/libs.src.js') }}"></script>
        <script src="{{ cachebust('/js/KaroApp.dev.js') }}"></script>
    {% endif %}
{% endblock %}
