{% extends 'base.html.twig' %}

{% block javascripts %}
    <script src="{{ cachebust('/js/KaroApp.src.js') }}"></script>
    <script>
        var app = new KaroApp({
            turtedHost: '{{ getConfig('turted') }}'
        });
        app.authUser.once('sync', () => {
            app.start();
        })
    </script>
{% endblock %}
