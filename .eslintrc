// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "padded-blocks": [
      2,
      {
        "classes": "always",
        "blocks": "never"
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "max-len": [
      "error",
      120
    ],
    "valid-jsdoc": "off"
  }
}
