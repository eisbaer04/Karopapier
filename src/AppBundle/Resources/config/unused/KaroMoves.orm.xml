<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\KaroMoves" table="karo_moves">
    <indexes>
      <index name="U_ID" columns="U_ID"/>
      <index name="G_U_ID" columns="G_ID,U_ID"/>
      <index name="idx_date" columns="date"/>
    </indexes>
    <id name="mId" type="integer" column="M_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="gId" type="integer" column="G_ID" nullable="false"/>
    <field name="uId" type="smallint" column="U_ID" nullable="false"/>
    <field name="date" type="datetime" column="date" nullable="false"/>
    <field name="xPos" type="smallint" column="x_pos" nullable="false"/>
    <field name="yPos" type="smallint" column="y_pos" nullable="false"/>
    <field name="xVec" type="boolean" column="x_vec" nullable="false"/>
    <field name="yVec" type="boolean" column="y_vec" nullable="false"/>
    <field name="crash" type="boolean" column="crash" nullable="false"/>
    <field name="movemessage" type="string" column="movemessage" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
