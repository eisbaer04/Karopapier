<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Game" table="karo_games">
    <indexes>
      <index name="U_ID" columns="U_ID"/>
    </indexes>
    <many-to-one target-entity="AppBundle\Entity\User" field="userId">
      <join-column name="U_ID" referenced-column-name="U_ID" />
    </many-to-one>
    <id name="id" type="integer" column="G_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="session" type="string" column="Session" length="255" nullable="true"/>
    <field name="name" type="string" column="Name" length="255" nullable="true"/>
    <field name="mapId" type="integer" column="M_ID" nullable="false"/>
    <field name="mailsent" type="boolean" column="mailsent" nullable="false"/>
    <field name="datemailsent" type="datetime" column="datemailsent" nullable="false"/>
    <field name="freeslots" type="integer" column="freeslots" nullable="false"/>
    <field name="comment" type="text" column="comment" length="65535" nullable="true"/>
    <field name="started" type="boolean" column="started" nullable="false"/>
    <field name="starteddate" type="datetime" column="starteddate" nullable="false"/>
    <field name="startedby" type="integer" column="startedby" nullable="false"/>
    <field name="finished" type="boolean" column="finished" nullable="false"/>
    <field name="finisheddate" type="datetime" column="finisheddate" nullable="false"/>
    <field name="zzz" type="integer" column="zzz" nullable="false"/>
    <field name="checkpoints" type="integer" column="checkpoints" nullable="false"/>
    <field name="crashallowed" type="integer" column="crashallowed" nullable="false"/>
    <field name="startdirection" type="integer" column="startdirection" nullable="false"/>
    <field name="isArchived" type="boolean" column="is_archived" nullable="false"/>
  </entity>
</doctrine-mapping>
