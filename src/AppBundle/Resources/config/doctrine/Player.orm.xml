<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Player" table="karo_teilnehmer">
    <indexes>
      <index name="U_ID" columns="U_ID"/>
    </indexes>
    <id name="gameId" type="integer" column="G_ID"/>
    <id name="userId" type="integer" column="U_ID"/>
    <field name="status" type="integer" column="status" nullable="false"/>
    <field name="moved" type="boolean" column="moved" nullable="false"/>
    <field name="finished" type="boolean" column="finished" nullable="false"/>
  </entity>
</doctrine-mapping>
